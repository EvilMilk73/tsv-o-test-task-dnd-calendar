{
  "version": 2,
  "builds": [
    {
      "src": "apps/testCalendarFront/vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "cd apps/testCalendarFront && npm install && npm run build"
      }
    },
    {
      "src": "apps/testCalendarBack/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/testCalendarBack/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/testCalendarFront/index.html"
    }
  ]
}
